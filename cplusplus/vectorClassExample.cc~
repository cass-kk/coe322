#include <iostream>
#include <cmath>
#include <vector>

using std::cout;
using std::endl;
using std::cin;
using std::vector;

int main(){

  vector<int> numbers = {1,-4,2,-6,};
  int num_max = 0;
  for(auto x: numbers){
    int absx = abs(x);
    if(absx > num_max){
      num_max = absx;
    }
  }
  cout << "Max: " << num_max << endl;

  return 0;
}
